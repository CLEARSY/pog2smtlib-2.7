MACHINE
    parallel_product_3
CONSTANTS
    co1,
    co2,
    co3,
    co4
PROPERTIES
    co1 = 10 |-> TRUE &
    co2 = 20 |-> 21 &
    co3 = 30 |-> 3.1 &
    co4 = 40 |-> 41
ASSERTIONS
    ({co1} || {co2}) = ({(10 |-> 20) |-> (TRUE |-> 21)});
    (({co1} || {co2}) || {co3}) =
    	   ({((10 |-> 20) |-> 30) |-> ((TRUE |-> 21) |-> 3.1)});
    ({co3} || {co4}) = ({(30 |-> 40) |-> (3.1 |-> 41)});
    (({co1} || {co2}) || ({co3} || {co4})) =
    	   ({((10 |-> 20) |-> (30 |-> 40)) |-> ((TRUE |-> 21) |-> (3.1 |-> 41))})
END
