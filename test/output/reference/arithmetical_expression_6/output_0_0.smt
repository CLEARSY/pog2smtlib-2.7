(set-option :print-success false)
(set-logic HO_ALL)
(define-sort |Z| () Int)
(declare-const c2 |Z|)
(declare-const c3 |Z|)
(declare-const c1 |Z|)

(declare-fun |int.div| (|Z| |Z|) |Z|)
(assert (!
  (forall ((a |Z|) (b |Z|))
    (and
      (=> (and (<= 0 a) (< 0 b))
        (= (|int.div| a b) (/ a b)))
      (=> (and (<= 0 a) (< b 0))
        (= (|int.div| a b) (- (/ a (- b)))))
      (=> (and (< a 0) (< 0 b))
        (= (|int.div| a b) (- (/ (- a) b))))
      (=> (and (<= a 0) (< b 0))
        (= (|int.div| a b) (/ a b)))))
  :named |ax.int.div :1|))
  (assert (!
  (not (= c1 (|int.div| c2 c3)))
  :named |Goal|))
(check-sat)
(exit)
